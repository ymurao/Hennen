		<gml:domainOfValidity/>

		<!-- 上記の編年要素を使った時間属性指定例（定義の際にも編年要素を利用している） -->
		<gml:component>
			<ChronologicalElement gml:id="AA00201">
				<gml:identifier codeSpace="url:nabunken.go.jp">C11</gml:identifier>
				<gml:name>明治中期～大正初期</gml:name>
				<termBegin>
					<ambiguousPosition>
						<estimatedTimeElement>
							<instant gml:id="AA00201a">
								<!-- gml:timePosition frame="#AA00101_Meiji">明治</gml:timePosition -->
								<gml:timePosition>明治</gml:timePosition>
							</instant>
							<timePartModifier>
								<nounPhrase>中期</nounPhrase>
								<functionPhrase>sqwave(1.0, 0.3, 0.4, 0.3)</functionPhrase>
								<description>明治時代中間前後40%の範囲</description>
							</timePartModifier>
						</estimatedTimeElement>

						<estimationMethod></estimationMethod>
					</ambiguousPosition>
				</termBegin>
				<termEnd>
					<ambiguousPosition>
						<estimatedTimeElement>
